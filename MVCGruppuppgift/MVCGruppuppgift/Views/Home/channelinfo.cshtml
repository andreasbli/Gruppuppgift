@model IEnumerable<MVCGruppuppgift.Models.program>

@{
    ViewBag.Title = "newsinfo";
}
@{
    var kanal = "";
}
@foreach (var i in Model)
{
    if (i.FK_channelid == 1)
    {
        kanal = "SVT1";
    }
    else if (i.FK_channelid == 2)
    {
        kanal = "SVT2";
    }
    else if (i.FK_channelid == 3)
    {
        kanal = "TV3";
    }
    else if (i.FK_channelid == 4)
    {
        kanal = "TV4";
    }
    else if (i.FK_channelid == 5)
    {
        kanal = "TV6";
    }
}
<div class="col-12" id="selectChannelInfo">

    <h1>@kanal</h1>
    <ul class="list-unstyled">

        @foreach (var item in Model)
        {
        <li>
            @item.date.Value.ToString("yyyy-MM-dd"),
            @item.time_from.Value.ToString(@"hh\:mm"),
            @item.name
        </li>
            <li>                
                @item.description
            </li>
        }
        </ul>

</div>